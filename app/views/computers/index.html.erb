<h1>Listado de computadores</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @computers.each do |computer| %>
      <tr>
        <td><%= computer.id %></td>
        <td><%= link_to computer.name, computer_path(computer) %></td>
        <td><%= computer.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_computer_path(computer), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', computer_path(computer), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_computer_path, :class => 'btn btn-primary' %>
